<h1>Admin Show</h1>
<%= link_to 'New', new_admin_path %>

<%= form_tag("/admins/login", method: "get") do %>
   <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </p>

  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>

  <%= submit_tag("Login") %>
<% end %>
